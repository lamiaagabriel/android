<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <VBox alignment="CENTER" spacing="15.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fitHeight="150" fitWidth="200" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../assets/icon.png" />
               </image>
            </ImageView>
            
            <ProgressIndicator prefHeight="30.0" prefWidth="30.0" style="-fx-accent: black;" />
         </children>
      </VBox>
   </center>
</BorderPane>
